<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<configuration>
	<!-- In dieser Datei schalten wir die Debug-Logger (die die Ereignisse in 
		den internen Funktionen der Bibliothek protokollieren) aus bzw. herunter. -->

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>%d{HH:mm:ss} %-5level %logger{36} - %msg%n</Pattern>
		</layout>
	</appender>

	<logger name="geneticlog" level="DEBUG" />
	<logger name="ownlog" level="WARN" />
	<root level="ERROR">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>